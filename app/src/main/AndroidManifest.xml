<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dk.aau.cs.psylog.light">
    <uses-permission android:name="dk.aau.cs.psylog.psylog.READ_DB"/>
    <uses-permission android:name="dk.aau.cs.psylog.psylog.WRITE_DB"/>
    <application android:allowBackup="true" android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher" android:theme="@style/AppTheme">
        <activity
            android:name="dk.aau.cs.psylog.light.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service
            android:name="dk.aau.cs.psylog.light.PsyLogService"
            android:label="Service"
            android:process=":my_process"
            android:exported="true">
        </service>
        <uses-feature android:name="android.hardware.sensor.light" android:required="true" />
    </application>

</manifest>
